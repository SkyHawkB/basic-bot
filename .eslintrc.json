{
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": [
    "json"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",2
    ],
    "linebreak-style": [
      "error", "windows"
    ],
    "quotes": [
      "error", "single"
    ],
    "semi": [
      "error", "always"
    ]
  }
}
